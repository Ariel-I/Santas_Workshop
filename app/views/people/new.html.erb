 <%= form_for(@person) do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name %><br><br>
    <%= f.label "Relationship" %>
    <%= f.text_field :relationship %><br><br>
    <%= f.label "Nice Rating" %>
    <%= f.text_field :nice_rating %><br><br>
    <%= f.label "Naughty Rating" %>
    <%= f.text_field :naughty_rating %><br><br>

    <%= f.submit "Submit" %>
  <% end %>